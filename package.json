{
  "name": "aimastery-vincian-analysis",
  "version": "7.1.0",
  "publisher": "Serigne-Diagne",
  "displayName": "AI Mastery: Vincian Analysis",
  "description": "Self-analyzing VS Code extension with integrated Local AI for intelligent code analysis",
  "icon": "images/logo-serigne-diagne-128px.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Serigne-Diagne/aimastery-vincian-analysis"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "ai",
    "analysis",
    "code-quality",
    "security",
    "performance",
    "typescript",
    "javascript"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:java",
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aimastery.selfAnalysis",
        "title": "üß¨ Analyze Self",
        "category": "AI Mastery",
        "icon": "$(symbol-structure)"
      },
      {
        "command": "aimastery.selfImprove",
        "title": "üîÑ Self-Improve",
        "category": "AI Mastery",
        "icon": "$(sync)"
      },
      {
        "command": "aimastery.realAnalysis",
        "title": "üéØ Analyze Current File",
        "category": "AI Mastery",
        "icon": "$(search)"
      },
      {
        "command": "aimastery.autoFix",
        "title": "üîß Auto-Fix Code Issues",
        "category": "AI Mastery",
        "icon": "$(tools)"
      },
      {
        "command": "aimastery.projectAnalysis",
        "title": "üìä Analyze Entire Project",
        "category": "AI Mastery",
        "icon": "$(graph)"
      },
      {
        "command": "aimastery.refreshData",
        "title": "Refresh Data",
        "icon": "$(refresh)"
      }
    ],
    "keybindings": [
      {
        "command": "aimastery.selfAnalysis",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s"
      },
      {
        "command": "aimastery.realAnalysis",
        "key": "ctrl+alt+a",
        "mac": "cmd+alt+a",
        "when": "editorTextFocus"
      },
      {
        "command": "aimastery.autoFix",
        "key": "ctrl+alt+f",
        "mac": "cmd+alt+f",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "aimastery.realAnalysis",
          "when": "editorHasSelection || editorTextFocus",
          "group": "1_modification@1"
        },
        {
          "command": "aimastery.autoFix",
          "when": "editorTextFocus",
          "group": "1_modification@2"
        }
      ],
      "explorer/context": [
        {
          "command": "aimastery.projectAnalysis",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace@1"
        }
      ],
      "commandPalette": [
        {
          "command": "aimastery.selfAnalysis"
        },
        {
          "command": "aimastery.selfImprove"
        },
        {
          "command": "aimastery.realAnalysis",
          "when": "editorTextFocus"
        },
        {
          "command": "aimastery.autoFix",
          "when": "editorTextFocus"
        },
        {
          "command": "aimastery.projectAnalysis"
        }
      ],
      "view/title": [
        {
          "command": "aimastery.refreshData",
          "when": "view == aimasteryFuzzy",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "AI Mastery",
      "properties": {
        "aimastery.autoAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic self-analysis on file changes"
        },
        "aimastery.autoImprovement": {
          "type": "boolean",
          "default": false,
          "description": "Enable automatic self-improvement (experimental)"
        },
        "aimastery.analysisDepth": {
          "type": "string",
          "enum": [
            "basic",
            "detailed",
            "comprehensive"
          ],
          "default": "detailed",
          "description": "Depth of code analysis"
        },
        "aimastery.improvementInterval": {
          "type": "number",
          "default": 3600,
          "minimum": 300,
          "maximum": 86400,
          "description": "Self-improvement interval in seconds (5min - 24h)"
        },
        "aimastery.healthThreshold": {
          "type": "number",
          "default": 0.8,
          "minimum": 0.1,
          "maximum": 1,
          "description": "Health score threshold for auto-improvement (0.1 - 1.0)"
        },
        "aimastery.patternRecognition": {
          "type": "boolean",
          "default": true,
          "description": "Enable pattern recognition and reproduction"
        },
        "aimastery.realTimeAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable real-time code analysis while typing"
        }
      }
    },
    "views": {
      "ai-mastery": [
        {
          "id": "aimastery-health",
          "name": "ü©∫ Code Health",
          "when": "aimastery.autoAnalysis"
        },
        {
          "id": "aimastery-patterns",
          "name": "üîÑ Detected Patterns",
          "when": "aimastery.patternRecognition"
        },
        {
          "id": "aimastery-improvements",
          "name": "üîß Applied Improvements",
          "when": "aimastery.autoImprovement"
        },
        {
          "id": "aimasteryFuzzy",
          "name": "üß¨ Fuzzy Analysis",
          "when": "workspaceFolderCount != 0"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-mastery",
          "title": "üß¨ AI Mastery",
          "icon": "$(symbol-structure)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.4",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=Serigne-Diagne.aimastery-vincian-analysis",
  "galleryBanner": {
    "color": "#2E86AB",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/Self--Analyzing-Code-2E86AB",
      "description": "Self-analyzing code extension"
    },
    {
      "url": "https://img.shields.io/badge/Real--Analysis-Enabled-4CAF50",
      "description": "Real code analysis and metrics"
    }
  ],
  "changelog": {
    "6.1.8": [
      "‚úÖ Fixed publication version conflict",
      "üé® Enhanced README with visual identity",
      "‚ö° Confirmed 24.5 KiB bundle optimization",
      "üñºÔ∏è Added professional logo integration",
      "üìä Updated performance benchmarks"
    ]
  }
}
