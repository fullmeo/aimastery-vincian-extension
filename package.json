{
  "name": "aimastery-vincian-analysis",
  "displayName": "AIMastery √ó FUZZY-SEA-QUEST",
  "description": "üåä Extension r√©volutionnaire de vibe-coding ! Analyse Vincienne + Gaming + Freemium",
  "version": "6.0.0",
  "publisher": "Serigne-Diagne",
  "keywords": [
    "fuzzy-sea-quest",
    "vibe-coding",
    "gaming",
    "vincian",
    "leonardo-da-vinci",
    "ai",
    "analysis",
    "freemium",
    "scorescout"
  ],
  "icon": "images/VinciRenaissance.png",
  // Ou utiliser votre logo personnel :
  // "icon": "images/logo-serigne-diagne-128px.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other",
    "Data Science",
    "Machine Learning",
    "Entertainment"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aimastery.vincianAnalysis",
        "title": "üé® Analyse Vincienne Enhanced",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.fuzzySync",
        "title": "üåä Sync FUZZY-SEA-QUEST",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.authenticate",
        "title": "üîê Connexion AIMastery",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.openGame",
        "title": "üéÆ Ouvrir FUZZY-SEA-QUEST",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.vibeMode",
        "title": "üéµ Changer Vibe Mode",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.healExtension",
        "title": "üîß Auto-Heal FUZZY",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.generateApp",
        "title": "üèóÔ∏è G√©n√©rer App IA",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.showDashboard",
        "title": "üìä Dashboard AIMastery",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.generateSocialContent",
        "title": "üì± Contenu Social AI",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.upgradeAccount",
        "title": "‚¨ÜÔ∏è Upgrade Premium",
        "category": "AIMastery"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "aimastery.vincianAnalysis",
          "when": "editorHasSelection",
          "group": "aimastery@1"
        }
      ],
      "commandPalette": [
        {
          "command": "aimastery.generateSocialContent",
          "when": "aimastery.authenticated"
        }
      ]
    },
    "configuration": {
      "title": "AIMastery FUZZY",
      "properties": {
        "aimastery.apiToken": {
          "type": "string",
          "description": "Token API AIMastery (depuis scorescout.eu/dashboard)"
        },
        "aimastery.gameUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "URL FUZZY-SEA-QUEST"
        },
        "aimastery.autoSync": {
          "type": "boolean",
          "default": true,
          "description": "Sync automatique avec FUZZY-SEA-QUEST"
        },
        "aimastery.vibeMode": {
          "type": "string",
          "enum": [
            "chill",
            "flow",
            "creative",
            "intense"
          ],
          "default": "flow",
          "description": "Mode de vibe actuel pour le coding"
        },
        "aimastery.plan": {
          "type": "string",
          "enum": [
            "free",
            "social_pack",
            "pro_vincien"
          ],
          "default": "free",
          "description": "Plan AIMastery actuel"
        },
        "aimastery.fuzzyServerPort": {
          "type": "number",
          "default": 8080,
          "description": "Port pour communication FUZZY-SEA-QUEST"
        }
      }
    },
    "views": {
      "explorer": [
        {
          "id": "aimasteryExplorer",
          "name": "üåä AIMastery FUZZY",
          "when": "aimastery.activated"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aimastery",
          "title": "AIMastery",
          "icon": "$(game)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish",
    "fuzzy-dev": "npm run compile && npm run package"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.100.0",
    "@types/ws": "^8.18.1",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullmeo/aimastery-vincian-extension.git"
  },
  "bugs": {
    "url": "https://github.com/fullmeo/aimastery-vincian-extension/issues"
  },
  "homepage": "https://scorescout.eu",
  "license": "MIT",
  "galleryBanner": {
    "color": "#20b2aa",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/FUZZY--SEA--QUEST-compatible-blue",
      "href": "https://github.com/fullmeo/fuzzy-octo",
      "description": "Compatible avec FUZZY-SEA-QUEST"
    }
  ],
  "directories": {
    "example": "examples"
  },
  "author": ""
}
