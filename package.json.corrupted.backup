{
  "name": "aimastery-vincian-analysis",
  "displayName": "AIMastery - √âcosyst√®me IA Cr√©atif",
  "description": "Extension VS Code connect√©e √† l'√©cosyst√®me AIMastery freemium - Analyse Vincienne + Social Media + NFT",
  "version": "5.2.0",
  "publisher": "Serigne-Diagne",
  "icon": "images/VinciRenaissance.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Data Science", 
    "Machine Learning",
    "Education"
  ],
  "keywords": [
    "ai",
    "artificial-intelligence", 
    "vincian",
    "leonardo-da-vinci",
    "code-analysis",
    "freemium",
    "social-media",
    "nft",
    "content-generation"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:r"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aimastery.authenticate",
        "title": "üîê Se connecter √† AIMastery",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.showDashboard",
        "title": "üìä Tableau de Bord AIMastery",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.generateContent",
        "title": "üé® G√©n√©rer du Contenu IA",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.analyzeVincian",
        "title": "üé≠ Analyse Vincienne du Code",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.showUpgradeOptions",
        "title": "‚¨ÜÔ∏è Upgrade vers Premium",
        "category": "AIMastery"
      },
      {
        "command": "aimastery.createNFT",
        "title": "üñºÔ∏è Cr√©er NFT Artwork",
        "category": "AIMastery"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "aimastery.analyzeVincian",
          "when": "editorHasSelection",
          "group": "aimastery@1"
        },
        {
          "command": "aimastery.generateContent",
          "when": "resourceExtname =~ /\\.(js|ts|py)$/",
          "group": "aimastery@2"
        }
      ],
      "commandPalette": [
        {
          "command": "aimastery.createNFT",
          "when": "aimastery.authenticated"
        }
      ]
    },
    "configuration": {
      "title": "AIMastery",
      "properties": {
        "aimastery.apiToken": {
          "type": "string",
          "description": "Token API AIMastery (depuis scorescout.eu)"
        },
        "aimastery.autoSync": {
          "type": "boolean",
          "default": true,
          "description": "Synchronisation automatique avec le cloud"
        },
        "aimastery.plan": {
          "type": "string",
          "enum": ["free", "social_pack", "pro_vincien"],
          "default": "free",
          "description": "Plan AIMastery actuel"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4",
    "vscode-test": "^1.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serigne-diagne/aimastery-vincian-analysis"
  },
  "bugs": {
    "url": "https://github.com/serigne-diagne/aimastery-vincian-analysis/issues"
  },
  "homepage": "https://scorescout.eu",
  "license": "MIT",
  "galleryBanner": {
    "color": "#667eea",
    "theme": "dark"
  }
}
